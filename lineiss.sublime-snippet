void lineiss(){
    string s;
    getline(cin,s);
    istringstream is(s);
    int x,y;
    string a,b;
    is>>a>>x>>b>>y;
    cout<<a<<endl;
    cout<<x<<endl;
    cout<<b<<endl;
    cout<<y<<endl;
}
